<tr>
  <td><%= ticket.route.name %></td>
  <td><%= ticket.train.number %></td>
  <td><%= ticket.seat %></td>
  <td><%= ticket.passanger_info %></td>
  <td><%= link_to t('common.show'), ticket_path(ticket), class: 'btn btn-sm btn-info'%>
    <%= link_to t('common.delete'), ticket_path(ticket), method: :delete, data: { confirm: t('common.delete_confirm') }, class: 'btn btn-sm btn-danger' %></td>

</tr>
